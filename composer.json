{
    "name" : "kachkaev/assets-version-bundle",
    "type" : "symfony-bundle",
    "description" : "Automates the process of updating assets version in Symfony2 & Symfony3 projects",
    "keywords" : [
        "assets",
        "assetic",
        "version",
        "autoincrement",
        "config",
        "console",
        "deploy"
    ],
    "homepage" : "http://github.com/kachkaev/KachkaevAssetsVersionBundle",
    "license" : "MIT",
    "authors" : [{
        "name" : "Alexander Kachkaev",
        "email" : "alexander@kachkaev.ru",
        "homepage" : "http://en.kachkaev.ru/"
    }, {
        "name" : "Contributors",
        "homepage" : "https://github.com/kachkaev/KachkaevAssetsVersionBundle/contributors"
    }
    ],
    "require" : {
        "php" : ">=5.3.2",
        "symfony/framework-bundle" : "~2.3|~3.0"
    },
    "require-dev" : {
        "symfony/filesystem" : "~2.3|~3.0",
        "symfony/finder" : "~2.3|~3.0",
        "satooshi/php-coveralls": "^2.1",
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.5 || ^7.5"
    },
    "scripts": {
        "coveralls": "vendor/bin/coveralls -v",
        "phpunit": "vendor/bin/phpunit"
    },
    "suggest" : {
        "symfony/assetic-bundle" : "For combining and compressing assets"
    },
    "autoload" : {
        "psr-4": {
            "Kachkaev\\AssetsVersionBundle\\": ""
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "2.0-dev"
        }
    }
}
